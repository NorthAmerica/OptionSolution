@*首页菜单*@
@model List<OP.Web.Models.MenuTreeModel>

<div id="aa" class="easyui-accordion" style="width:253px;">
    @if (Model.Count>0)
    {
        foreach (var item in Model)
        {
            <div title="@Html.Encode(item.MenuName)" data-options="iconCls:'icon-scategorys',selected:true" style="overflow:auto;padding:10px;">
                
                @{
                    if (item.children!=null)
                    {
                        Html.RenderPartial("HomeMenuChild", item.children);
                    }
                }
            </div>
        }
    }
    
</div> 
